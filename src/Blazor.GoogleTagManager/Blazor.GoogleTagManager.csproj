	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<NoWarn>1591;1701;1702;SA1134</NoWarn>
		<Nullable>enable</Nullable>
		<TrimMode>link</TrimMode>
		<IsTrimmable>true</IsTrimmable>
